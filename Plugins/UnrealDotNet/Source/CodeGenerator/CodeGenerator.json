            "FQueuedDemoPacket",
            "FNetGUIDCache",

            "UAssetManager",

            "FQueuedReplayTask", // todo: не экспортировать абстрактные структруты

            "APlanarReflection", // невозможно преобразовать "UPlanarReflectionComponent *" в "UObject *", но почему?
