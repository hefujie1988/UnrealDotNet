        "ReadOnlyClass": [
            "FTickFunction" // TODO: находить удаленный оператор присваивания
        ],
        "UseConvertToManageType": {
            "TCHAR": "StringWrapper",
            "FName": "StringWrapper",
            "FText": "StringWrapper",
            "FString": "StringWrapper"
        },
        "UseConvertFromManageType": {
            "TCHAR": "char*",
            "FName": "char*",
            "FText": "char*",
            "FString": "char*"
        },
        "MethodInClassBlackList": {
            "UObjectBase": [
